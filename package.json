    "format:fix": "turbo run format:fix",
    "clean": "turbo run clean",
    "generate-keys": "dotenv -- turbo run generate-keys"
  },
  "devDependencies": {
    "dotenv-cli": "^11.0.0",
    "jiti": "^2.6.1",
    "turbo": "^2.6.0"
  }
}